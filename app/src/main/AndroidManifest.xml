<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hoandeptraicompany.com.demngayfa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.FlashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.MainActivity" />
        <activity
            android:name=".View.ChangeColorActivity"
            android:theme="@style/Theme.AppCompat.Light.Dialog" />
        <activity
            android:name=".View.ChangeTopTitleActivity"
            android:theme="@style/MyDiaLog" />
        <activity
            android:name=".View.ChangeBottomTitleActivity"
            android:theme="@style/MyDiaLog" />
        <activity android:name=".View.ChangeBackgroundActivity" />
    </application>

</manifest>